<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="jlanhousemanagerPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>model.Cliente</class>
    <class>model.DespesaFixa</class>
    <class>model.DespesaVariavel</class>
    <class>model.Funcionario</class>
    <class>model.Produto</class>
    <class>model.Fornecedor</class>
    <class>model.Aquisicao</class>
    <class>model.TipoDespesa</class>
    <class>model.ItemAquisicao</class>
    <class>model.Computador</class>
    <class>model.ExecucaoDeServico</class>
    <class>model.Conta</class>
    <class>model.EspecificacaoServico</class>
    <class>model.CompraDeProduto</class>
    <class>model.ItemDaConta</class>
    <class>model.UsoDoComputador</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost/dblanhouse"/>
      <property name="javax.persistence.jdbc.password" value="12345"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
    </properties>
  </persistence-unit>
</persistence>
